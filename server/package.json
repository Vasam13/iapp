    "generate-app-hook": "node -r ts-node/register -r tsconfig-paths/register src/core/utils/GenerateAppScript",
    "merge-scripts": "node _merge_scripts.js",
    "main": "node -r ts-node/register -r tsconfig-paths/register src/core/utils/Main"
  },
